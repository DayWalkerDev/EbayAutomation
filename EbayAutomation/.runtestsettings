<!-- multiple .runsettings files can be added for parallel run on different browsers -->
<RunSettings>
  <Playwright>
    <BrowserName>chromium</BrowserName>
    <LaunchOptions>
      <Headless>true</Headless> 
      <SlowMo>5000</SlowMo>     <!-- 5 second delay between actions, overrides bot check -->
    </LaunchOptions>
  </Playwright>
</RunSettings>